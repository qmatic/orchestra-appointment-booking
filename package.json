{
  "name": "staff-booking",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run ng-config-dev && ng serve --proxy-config proxy.config.json",
    "startlocal": "npm run ng-config-dev && ng serve --proxy-config proxy.config.localhost.json",
    "build": "ng build --prod",
    "test": "ng test",
    "build-war-dev": "npm run ng-config-dev && ng build && gulp build:war:properties",
    "build-war-prod": "npm run ng-config-prod && ng build --prod --base-href /appointmentbooking/ && gulp build:war:properties",
    "build-artifactory": "npm run ng-config-prod && ng build --prod --base-href /appointmentbooking/ && gulp build:artifactory",
    "deploy-war-dev": "npm run ng-config-dev && ng build && gulp build:war:deploy",
    "deploy-war-prod": "npm run ng-config-prod && ng build --prod --base-href /appointmentbooking/ && gulp build:war:deploy",
    "deploy-lang": "gulp deploy:lang",
    "deploy-artifactory": "npm run ng-config-prod && ng build --prod --base-href /appointmentbooking/ && gulp build:artifactory:deploy",
    "ng-config-dev": "node build-configuration/build-config.js dev",
    "ng-config-prod": "node build-configuration/build-config.js prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.5",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@ng-bootstrap/ng-bootstrap": "^2.0.0",
    "@ng-select/ng-select": "^1.1.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "@types/jquery": "^3.3.1",
    "angular-moment-timezone": "^1.7.1",
    "angular2-moment": "^1.9.0",
    "bootstrap": "^4.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "jquery": "^3.3.1",
    "ng2-bootstrap-modal": "^1.0.1",
    "ngx-toastr": "^8.4.0",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
