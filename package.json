    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-sftp": "^0.1.5",
    "gulp-sync": "^0.1.4",
    "gulp-zip": "^4.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
